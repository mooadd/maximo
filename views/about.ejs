              </div>
              <!-- END SIDEBAR BUTTONS -->
              <!-- SIDEBAR MENU -->
              <div class="wrapper">
                <ui class="mainMenu">
                    <li class="item" id="account">
                        <a href="#account" class="btn-menu"><i class="fas fa-user-circle"></i>Switch Account</a>
                        <div class="subMenu">
                            <a href="">item-1</a>
                            <a href="">item-2</a>
                            <a href="">item-3</a>
                        </div>
                    </li>
                    <li class="item" id="about">
                        <a href="#about" class="btn-menu"><i class="fas fa-address-card"></i>About</a>
                        <div class="subMenu">
                            <a href="">item-1</a>
                            <a href="">item-2</a>
                        </div>
                    </li>
                    <li class="item" id="support">
                        <a href="#support" class="btn-menu"><i class="fas fa-info"></i>Support</a>
                        <div class="subMenu">
                            <a href="">item-1</a>
                        </div>
                    </li>
                    <li class="item">
                        <a href="" class="btn-menu"><i class="fas fa-sign-out-alt"></i>Log Out</a>
                    </li>
                </ui>
            </div>
              <!-- END MENU -->
            </div>
          </div>
          <div class="col-md-9">
            <div class="profile-content">
              Profile info...

            </div>
          </div>
        </div>
      </div>
